{
  "name": "lucide-icons-qwik",
  "version": "0.525.0",
  "description": "Lucide icon library package for Qwik applications",
  "author": {
    "name": "Luminescent",
    "url": "https://luminescent.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LuminescentDev/lucide-icons-qwik.git"
  },
  "license": "LICENSE",
  "keywords": [
    "icon",
    "icons",
    "lucide",
    "qwik"
  ],
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": ">=16.8.0 <18.0.0 || >=18.11",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8.6.12"
  },
  "packageManager": "pnpm@10.13.1",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "pnpm build.icons && qwik build",
    "build.icons": "node --trace-warnings build.js",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "np",
    "start": "vite --open --mode ssr",
    "sync-version": "node --trace-warnings sync-version.js",
    "qwik": "qwik",
    "test": "echo no tests for now"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.107.0",
    "@eslint/js": "^9.31.0",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.17",
    "eslint": "9.31.0",
    "eslint-plugin-qwik": "^0.107.0",
    "globals": "^13.19.0",
    "lucide": "^0.525.0",
    "np": "^7.7.0",
    "typescript": "4.9.5",
    "typescript-eslint": "^8.38.0",
    "undici": "5.29.0",
    "vite": "4.5.14",
    "yargs": "^18.0.0"
  }
}
